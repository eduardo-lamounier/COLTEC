// Lab3: Codigo do professor - usar para consultar algo.

#include<stdio.h>

int main(){
	printf("Essa é uma string simples\n");
	
	char *minha_string = "não é uma string";
	printf("Essa %s simples\n", minha_string);

	int a = 1;
	int b = 5;
	int c;

	c = a + b;

	printf("O primeiro numero é %d\n", a);
	printf("O segundo numero é %d\n", b);
	printf("O terceiro numero é %d\n", c);
}
